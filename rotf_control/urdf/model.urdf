            <mass value="15"/>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <inertia ixx="1.0" ixy="0" ixz="0" iyy="0.0" iyz="1.0" izz="0.0"/>
        </inertial>
        <visual name="base_link_visual" >
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size=".5 .3 .1"/>
            </geometry>
        </visual> 
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size=".5 .3 .1"/>
            </geometry>
        </collision>
        <collision name="caster_front_collision">
            <origin rpy=" 0 0 0" xyz="0.35 0.0 -0.05"/>
            <geometry>
                <sphere radius="0.05"/>
            </geometry>
            <surface>
                <friction>
                <ode>
                    <mu>0</mu>
                    <mu2>0</mu2>
                    <slip1>1.0</slip1>
                    <slip2>1.0</slip2>
                </ode>
                </friction>
            </surface>
        </collision>
        <visual name="caster_front_visual">
            <origin rpy=" 0 0 0" xyz="0.2 0 -0.05"/>
            <geometry>
                <sphere radius="0.05"/>
            </geometry>
        </visual>  
    </link>
    
    <gazebo reference="base_link">
    		<material>Gazebo/White</material>
            <turnGravityOff>false</turnGravityOff>
  	</gazebo>
