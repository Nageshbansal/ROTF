    <link name="left_wheel">
        <inertial>
            <mass value="1"/>
            <origin xyz="-0.1 0.0 0.0" rpy="0.0 0 0"/>
            <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
        </inertial>
        <visual name="left_wheel_visual" >
            <origin xyz="0.0 0.0 0.0" rpy="1.5707 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.1" length="0.03"/>
            </geometry>
            <material name="white">
                <color rgba="1.0 0.0 0.0 1.0"/>
            </material>
        </visual>
        <collision name="link_left_wheel_collision">
            <origin xyz="0.0 0.0 0.0" rpy="1.5707 0 0"/>
            <geometry>
                <cylinder radius="0.1" length="0.03"/>
            </geometry>
        </collision>
    </link>
    <joint name="left_wheel_joint" type="continuous">
        <origin xyz="-0.15 -0.15 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="left_wheel"/>
        <axis xyz="0.0 1.0 0.0"/>
        <update_rate>20</update_rate>
        <limit effort="100.0" velocity="100.0"/>
        <joint_properties damping="0.0" friction="0.0"/>
    </joint>
    <transmission name="tran_left">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="left_wheel_joint">
            <hardwareInterface>EffortJointInterface</hardwareInterface>
        </joint>
        <actuator name="motor_left">
            <hardwareInterface>EffortJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>
     <gazebo reference="left_wheel">
        <mu1 value="1.0"/>
        <mu2 value="1.0"/>
        <update_rate>20</update_rate>
        <kp value="1.0"/>
        <kd value="00.0"/>
        <ki value="0.0"/>
        <fdir1 value="1 0 0"/>
        <material>Gazebo/Grey</material>
        <turnGravityOff>false</turnGravityOff>
    </gazebo>
    <link name="right_wheel">
        <inertial>
            <mass value="1"/>
            <origin xyz="-0.1 0.0 0.0" rpy="0.0 0 0"/>
            <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
        </inertial>
        <visual name="right_wheel_visual" >
            <origin xyz="0.0 0.0 0.0" rpy="1.5707 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.1" length="0.03"/>
            </geometry>
            <material name="white">
                <color rgba="1.0 0.0 0.0 1.0"/>
            </material>
        </visual>
        <collision name="link_right_wheel_collision">
            <origin xyz="0.0 0.0 0.0" rpy="1.5707 0 0"/>
            <geometry>
                <cylinder radius="0.1" length="0.03"/>
            </geometry>
        </collision>
    </link>
    <joint name="right_wheel_joint" type="continuous">
        <origin xyz="-0.15 0.15 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="right_wheel"/>
        <update_rate>20</update_rate>
        <axis xyz="0.0 1.0 0.0"/>
        <limit effort="100.0" velocity="100.0"/>
        <joint_properties damping="0.0" friction="0.0"/>
    </joint>
    <transmission name="tran_right">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="right_wheel_joint">
            <hardwareInterface>EffortJointInterface</hardwareInterface>
        </joint>
        <actuator name="motor_right">
            <hardwareInterface>EffortJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>
